%define __STR__()
std::string __str__() {
  std::ostringstream out;
  out << *$self;
  return out.str().c_str();
}
%enddef

%define __REPR__()
std::string __repr__() {
  std::ostringstream out;
  out << "<" << *$self << ">";
  return out.str().c_str();
}
%enddef

%extend Link{
    __STR__();
    __REPR__();
}

%extend Node{
    __STR__();
    __REPR__();
}

%extend LongReadMapping{
    __STR__();
    __REPR__();
};

%extend KmerIDX{
    __STR__();
    __REPR__();
};

%extend MinPosIDX{
    __STR__();
    __REPR__();
};

%extend nodeVisitor{
    __STR__();
    __REPR__();
};

